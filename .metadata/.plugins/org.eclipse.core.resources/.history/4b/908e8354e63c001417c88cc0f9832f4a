
public class ArrayDemo {

	public static void main(String[] args) {
		int[] anArray;
		
		anArray = new int[10];
		
		anArray[0] = 100;
		anArray[1] = 200;
		anArray[2] = 300;
		anArray[3] = 400;
		anArray[4] = 500;
		anArray[5] = 600;
		anArray[6] = 700;
		anArray[7] = 800;
		anArray[8] = 900;
		anArray[9] = 1000;
		
		System.out.println("Element at index 0 is: " + anArray[0]);
		System.out.println("Element at index 0 is: " + anArray[1]);
		System.out.println("Element at index 0 is: " + anArray[2]);
		System.out.println("Element at index 0 is: " + anArray[3]);
		System.out.println("Element at index 0 is: " + anArray[4]);
		System.out.println("Element at index 0 is: " + anArray[5]);
		System.out.println("Element at index 0 is: " + anArray[6]);
		System.out.println("Element at index 0 is: " + anArray[7]);
		System.out.println("Element at index 0 is: " + anArray[8]);
		System.out.println("Element at index 0 is: " + anArray[9]);
	}

}
